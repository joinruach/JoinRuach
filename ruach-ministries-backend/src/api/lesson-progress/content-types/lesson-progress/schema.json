{
  "kind": "collectionType",
  "collectionName": "lesson_progresses",
  "info": {
    "singularName": "lesson-progress",
    "pluralName": "lesson-progresses",
    "displayName": "Lesson Progress"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "courseSlug": {
      "type": "string",
      "required": true
    },
    "lessonSlug": {
      "type": "string",
      "required": true
    },
    "secondsWatched": {
      "type": "integer",
      "default": 0
    },
    "progressPercent": {
      "type": "integer",
      "default": 0
    },
    "completed": {
      "type": "boolean",
      "default": false
    },
    "completedAt": {
      "type": "datetime"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
