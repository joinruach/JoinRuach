{
  "kind": "collectionType",
  "collectionName": "outreach_stories",
  "info": {
    "singularName": "outreach-story",
    "pluralName": "outreach-stories",
    "displayName": "Outreach Story",
    "description": "Stories and testimonies from community outreach"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "storyDate": {
      "type": "date"
    },
    "summary": {
      "type": "text"
    },
    "body": {
      "type": "richtext"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "videos",
        "files"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "relatedCampaign": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::outreach-campaign.outreach-campaign",
      "inversedBy": "stories"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
