{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "bio": {
      "type": "blocks"
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "socialLinks": {
      "type": "component",
      "repeatable": true,
      "component": "general.social-links"
    },
    "blog_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "team_member"
    },
    "images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::image.image",
      "mappedBy": "team_member"
    }
  }
}
