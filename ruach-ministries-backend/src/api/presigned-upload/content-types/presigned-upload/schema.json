      "required": true
    },
    "mimeType": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "biginteger",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "uploadedAt": {
      "type": "datetime"
    }
  }
