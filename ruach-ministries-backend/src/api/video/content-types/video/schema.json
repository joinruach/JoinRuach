{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video (read-only)",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "videoUrl": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "videos"
    },
    "thumbnail": {
      "allowedTypes": [
        "files",
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "videos"
    },
    "publishedDate": {
      "type": "date"
    },
    "channels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::channel.channel",
      "inversedBy": "videos"
    },
    "isShort": {
      "type": "boolean",
      "default": false
    },
    "uid": {
      "type": "uid"
    },
    "trending_video": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "unique": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    }
  }
}
