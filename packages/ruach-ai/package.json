{
  "name": "@ruach/ai",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./embeddings": {
      "types": "./dist/embeddings/index.d.ts",
      "default": "./dist/embeddings/index.js"
    },
    "./chat": {
      "types": "./dist/chat/index.d.ts",
      "default": "./dist/chat/index.js"
    },
    "./recommendations": {
      "types": "./dist/recommendations/index.d.ts",
      "default": "./dist/recommendations/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "openai": "^6.8.1",
    "@ai-sdk/anthropic": "^2.0.44",
    "ai": "^5.0.92",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "tsup": "^8.1.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.8"
  }
}
