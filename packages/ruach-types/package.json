{
  "name": "@ruach/types",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "typecheck": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  }
}
